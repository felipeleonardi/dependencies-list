//                      "directory": ""
//                  }
//              }
            ],


            "scriptVerification": [
                {
                    "name": "SV Auto Instance",
                    "enable": false,
                    "excludeScripts": [],
//                  "includeScripts": [],
                    "externalScripts": [],
                    "validateExternalScriptUsage": true,
                    "mode": "batch",
                    "tamperAction": "",
                    "nonTamperAction": "",
                    "immediateAction": false,
                    "invocationLocations": "auto"
                },
//              {
//                  "name": "SV Function Invoked Instance",
//                  "enable": false,
//                  "excludeScripts": [],
