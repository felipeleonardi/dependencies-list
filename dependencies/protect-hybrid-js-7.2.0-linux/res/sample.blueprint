{
    "globalConfiguration": {
        "seed": "random",
        "targetType": "reactnative-ios",
        "runtimeEnvironmentCheck": false,
        "autoIgnoreIdentifiers": true,

//      "importFile": "",
//      "exportFile": "",

        "preserveFunctions": false,

//      Please refer to documentation if you want to use ephemeral mode.
//      "ephemeralMode": "",
        "licenseRegion": "NorthAmerica",

//      appID defines how the applications App Aware data is stored.
//      The value is auto-generated based on input files if left unspecified.
//      "appID": "myAppID",

        "ignoreGlobals": false
    },

    "debugMode": {
        "enable": false,
        "refreshRate": 3000,
        "domOutput": false
    },

    "appAware": {
        "enable": true,
        "endpointURL": "auto",
        "applicationToken": "auto",
        "getInstallIDCallback": "",
        "mappingFileOutputFolder": "",
        "fullReporting": false,
//      "appInstanceIdentifierGetter": "getAppIdentifier",

//      Custom data fields can be added to all App Aware messages.
//      Please refer to documentation if you want to use this feature.
//      "customDataFields": {
//          "fieldName": {
//              "valueCallback": "callbackFunctionName"
//          }
//      }
    },

    "reactions": {
        "myCallbackAction": {
            "type": "callback",
            "function": "myFunction",
            "validate": false,
            "promise": false
        },

//      Only applicable when target type is Cordova-Android or Cordova-iOS
//      "myRedirectAction": {
//          "type": "redirect",
//          "url": "./pagenotfound.html",
//          "allowReturn": false
//      },

//      Only applicable when target type is Cordova-Android or Cordova-iOS
//      "myDeleteCookiesAction": {
//          "type": "deleteCookies",
//          "cookies": [ "cookie1", "cookie2" ]
//      },

        "myEmptyAction": {
            "type": "noAction"
        },

        "myTerminateAppAction": {
            "type": "terminateApp",
        }
    },

    "invocationLocations": {
        "invocationFromFunction": {
            "type": "function",
            "name": "invocationFunctionName",
            "validate": false
        },

        "autoInvocation": {
            "type": "auto"
        }
    },

    "debugTiming": {
        "enable": false,
        "initialDelay": 1,
        "interval": 5
    },

    "guardConfigurations": {
        "guard_configuration_1": {
            "protectionPreset": "normal",
            "identifierRenaming": {
                "enable": true,                
                "scheme": "short",
                "characterSet": "Ascii-Extended",
                "ignore": [],
                "mapping": {},
//              "modifier": {
//                  "positions": ["prefix", "postfix", "infix"],
//                  "strings": ["ab", "cd", "ef"],
//                  "scope": "global" // "all"
//              }
            },

            "stringEncryption": {
                "enable": true,
                "forceStrong": [],
                "forceFast": [],
                "exclude": [],
                "protectionCoverage": 90,
                "protectionStrength": 5,
                "cachePercentage": 100,
                "maxLength": 64,
                "enforceContext": false,
                "uniqueCacheIdentifiers": true,
                "variations": "full"
            },

            "algorithmCorruption": {
                "enable": false,
                "coverage": 20
            },

            "operatorRemoval": {
                "enable": true,
                "protectionCoverage": 90,
                "protectionStrength": 5
            },

            "externalHiding": {
                "enable": true
            },

            "numericLiteralHiding": {
                "enable": true,
                "extendedNumericLiteralHiding": true,
                "protectionCoverage": 90,
                "protectionStrength": 5,
                "cachePercentage": 100,
                "uniqueCacheIdentifiers": true
            },

            "controlFlowFlattening": {
                "enable": true,
                "mode": "Basic",
//              "functionalStatementCount": 5,
//              "protectionStrength": 0,
                "targetCaseCount": 10,
                "minFunctionSize": 3
            },

            "dataIntegrity": {
                "enable": false
            },

            "checksum": [
                {
                    "name": "Checksum First Instance",
                    "enable": false,
                    "tamperAction": "",
                    "nonTamperAction": "",
                    "excludeFunctions": [],
                    "includeFunctions": [],
                    "protectionCoverage": 20,
                    "inliningLevel": 5,
                    "validityPeriod": 1000,
                    "immediateAction": false,
                    "mapping": {
                        "enable": false,
                        "directory": ""
                    }
                },
//              {
//                  "name": "Checksum Second Instance",
//                  "enable": false,
//                  "tamperAction": "",
//                  "nonTamperAction": "",
//                  "excludeFunctions": [],
