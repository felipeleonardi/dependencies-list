                    "name": "DD Auto Instance",
                    "enable": false,
                    "immediateAction": false,
                    "infiniteDebuggerLoop": true,
                    "tamperAction": "",
                    "nonTamperAction": "",
//                  Allowed only on cordova (browser-like) targets
                    "detectionTechnique": "global",
                    "invocationLocations": "auto"
                },
